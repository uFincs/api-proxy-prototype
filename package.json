{
    "name": "ufincs-api-proxy-prototype",
    "version": "0.0.1",
    "description": "A prototype of an API proxy for uFincs.",
    "main": "src/index.js",
    "author": "Devin Sit <devin@ufincs.com>",
    "license": "MIT",
    "engines": {
        "node": ">=12.0.0"
    },
    "scripts": {
        "test": "npm run jest",
        "start": "NODE_PATH=src ts-node-dev --no-notify src/",
        "start:prod": "NODE_PATH=lib node lib/",
        "compile": "shx rm -rf lib/ && tsc",
        "tsc:check": "tsc --noEmit true",
        "lint": "NODE_PATH=src eslint .",
        "prettier": "prettier --write src/**/*.js",
        "prettier:check": "prettier --check src/**/*.js"
    },
    "standard": {
        "env": [
            "jest"
        ],
        "ignore": []
    },
    "types": "lib/",
    "dependencies": {
        "@feathersjs/client": "4.5.4",
        "@reduxjs/toolkit": "^1.8.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.17.1",
        "express-rate-limit": "^5.1.3",
        "fast-text-encoding": "^1.0.3",
        "helmet": "^4.1.0",
        "http-status": "^1.4.0",
        "isomorphic-fetch": "^3.0.0",
        "isomorphic-webcrypto": "2.3.8",
        "jsonwebtoken": "^8.5.1",
        "localforage": "1.9.0",
        "morgan": "^1.10.0",
        "redux": "^4.0.5",
        "winston": "^3.2.1",
        "xss-clean": "^0.1.1"
    },
    "devDependencies": {
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.12",
        "@types/helmet": "^4.0.0",
        "@types/jest": "^25.2.3",
        "@typescript-eslint/eslint-plugin": "^4.8.2",
        "@typescript-eslint/parser": "^4.8.2",
        "eslint": "^7.1.0",
        "eslint-plugin-import": "^2.20.2",
        "jest": "^26.0.1",
        "nodemon": "^2.0.4",
        "prettier": "^2.3.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.1.2"
    }
}
